    "image-size": "1.0.2",
    "mdx-bundler": "^9.0.1",
    "next": "^12.3.1",
    "next-images": "^1.8.4",
    "next-themes": "^0.2.0",
    "postcss": "^8.4.16",
    "preact": "^10.11.0",
    "react": "^18.1.0",
